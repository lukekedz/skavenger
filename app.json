{
  "expo": {
    "name": "skavenger",
    "slug": "skavenger",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/raven.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/raven.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ios.skavenger"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/raven.png",
        "backgroundColor": "#ffffff"
      },
      "package": "ios.skavenger"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/raven.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e8026405-513e-4f5d-9afc-461e6dd3d598"
      }
    }
  }
}
